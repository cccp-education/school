{
  "name": "talaria-frontend",
  "version": "0.0.1",
  "description": "Frontend of the Talaria project using next.js, cucumber-js, Playwright, Storybook",
  "author": "cheroliv",
  "license": "ISC",
  "homepage": "https://github.com:cheroliv/formateur#README",
  "repository": {
    "type": "git",
    "url": "git@github.com:cheroliv/formateur.git"
  },
  "bugs": {
    "url": "https://github.com:cheroliv/formateur/issues"
  },
  "main": "index.js",
  "scripts": {
    "predeploy": "next build",
    "dev": "next dev",
    "test": "npm run unit-test && npm run cucumber-check",
    "unit-test": "npx playwright test --workers 3",
    "start": "next start",
    "api": "DEBUG=pw:api cucumber-js",
    "build": "rimraf build && npm run format && npm run predeploy",
    "cucumber-check": "cucumber-js behavior/features/**/*.feature --dry-run --require env/set-environment-variables.ts --require behavior/steps/**/*.ts --require behavior/support/**/*.ts --require behavior/utils/**/*.ts  --require-module ts-node/register --format summary --format progress --format progress-bar  --format-options \"{\\\"snippetInterface\\\": \\\"async-await\\\"}\"",
    "debug": "PWDEBUG=1 DEBUG=pw:api cucumber-js",
    "video": "PWVIDEO=1 cucumber-js",
    "eslint-fix": "eslint ./ --ext .js,.ts,.tsx --fix",
    "eslint-init": "eslint --init",
    "format": "prettier --write \"**/*.{ts,tsx,css,html}\" ",
    "lint": "eslint ./ --ext .js,.ts,.tsx --format visualstudio --no-color --report-unused-disable-directives && next lint",
    "only": "npm run cucumber -- --tags @only",
    "report": "open reports/report.html",
    "snippets": "cucumber-js behavior/features/**/*.feature --dry-run --format snippets",
    "steps-usage": "cucumber-js behavior/features/**/*.feature --dry-run",
    "all": "cucumber-js behavior/features/**/*.feature",
    "test:parallel": "cucumber-js --parallel=2",
    "docker": "docker run --rm --network host -v $(pwd):/work/ -w /work/ -it mcr.microsoft.com/playwright:latest /bin/bash",
    "allure": "allure serve reports/allure-results",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@cucumber/cucumber": "10.0.1",
    "@cucumber/html-formatter": "21.0.0",
    "@cucumber/messages": "^24.0.0",
    "@cucumber/pretty-formatter": "1.0.0",
    "@playwright/test": "1.40.1",
    "@types/fs-extra": "11.0.4",
    "cucumber-console-formatter": "1.0.0",
    "cucumber-html-reporter": "^6.0.0",
    "pixelmatch": "5.3.0",
    "pngjs": "7.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.5.0",
    "@storybook/addon-essentials": "^8.1.10",
    "@storybook/addon-interactions": "^8.1.10",
    "@storybook/addon-links": "^8.1.10",
    "@storybook/addon-mdx-gfm": "^8.1.10",
    "@storybook/addon-onboarding": "^8.1.10",
    "@storybook/blocks": "^8.1.10",
    "@storybook/nextjs": "^8.1.10",
    "@storybook/react": "^8.1.10",
    "@storybook/test": "^8.1.10",
    "@types/expect": "24.3.0",
    "@types/fs-extra": "11.0.4",
    "@types/lodash": "4.14.202",
    "@types/node": "20.10.2",
    "@types/pixelmatch": "5.2.6",
    "@types/pngjs": "6.0.4",
    "@types/react": "18.2.47",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "allure-cucumberjs": "2.9.2",
    "autoprefixer": "^10.4.16",
    "eslint": "8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-storybook": "^0.6.15",
    "fp-ts": "^2.16.2",
    "fs-extra": "11.2.0",
    "next": "^14.0.4",
    "open": "9.1.0",
    "postcss": "^8.4.33",
    "prettier": "3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "6.0.1",
    "standard-version": "9.5.0",
    "storybook": "^8.1.10",
    "tailwindcss": "^3.4.1",
    "ts-node": "10.9.1",
    "typescript": "5.3.2"
  }
}
